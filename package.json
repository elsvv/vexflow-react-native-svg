{
    "name": "@eliseev_s/vexflow-react-native-svg",
    "version": "1.0.0",
    "description": "React Native SVG rendering backend for VexFlow - render music notation in React Native apps",
    "main": "lib/index.js",
    "module": "lib/index.js",
    "types": "lib/index.d.ts",
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./lib/index.d.ts",
            "import": "./lib/index.js",
            "require": "./lib/index.js"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/elsvv/vexflow-react-native-svg.git"
    },
    "bugs": {
        "url": "https://github.com/elsvv/vexflow-react-native-svg/issues"
    },
    "homepage": "https://github.com/elsvv/vexflow-react-native-svg#readme",
    "keywords": [
        "vexflow",
        "react-native",
        "svg",
        "music",
        "notation",
        "sheet-music",
        "tablature",
        "guitar",
        "piano"
    ],
    "author": "Vacheslav Eliseev",
    "license": "MIT",
    "peerDependencies": {
        "react": ">=18.0.0",
        "react-native": ">=0.72.0",
        "react-native-svg": ">=15.15.0",
        "vexflow": ">=5.0.0"
    },
    "peerDependenciesMeta": {
        "react-native": {
            "optional": false
        },
        "react-native-svg": {
            "optional": false
        }
    },
    "devDependencies": {
        "@types/react": "^19.0.0",
        "react": "^19.2.0",
        "react-native": "^0.83.0",
        "react-native-svg": "^15.15.0",
        "typescript": "^5.9.0",
        "vexflow": "^5.0.0",
        "vitest": "^2.0.0"
    },
    "scripts": {
        "build": "tsc",
        "clean": "rm -rf lib",
        "test": "vitest run",
        "test:watch": "vitest",
        "prepublishOnly": "npm run clean && npm run build",
        "typecheck": "tsc --noEmit"
    },
    "files": [
        "lib",
        "README.md"
    ],
    "publishConfig": {
        "access": "public"
    }
}
